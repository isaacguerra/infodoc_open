var x;
x=$(document);
x.ready(inicializarEventos);

function inicializarEventos()
{
  var x=$("#login_usuario");
  x.focus(tomaFoco);
  x.blur(pierdeFoco);
  x=$("#login_senha");
  x.focus(tomaFoco);
  x.blur(pierdeFoco);
}

function tomaFoco()
{
  var x=$(this);
  x.css("background","#ffffff");
}

function pierdeFoco()
{
  var x=$(this);
  x.css("background","#F5F5F5");
}

