<div id="barra_titulo">
  <h3 class="titulo_index">InfoWiki</h3>
</div>
<table id="listar">
  </tr>
    <th class="titulo_lista">InfoWiki</th>
    <th class="titulo_lista">
        <div id="link_botao"><%= link_to_remote "Nova Categorias", :url=>(url_for(new_wiki_wikicategoria_path)), :method=>"get" %></div>
        Categorias
    </th>
  </tr>
  <tr>
    <td>
      <h3>Busca no Conte√∫do Wiki</h3>
      <% form_remote_tag :url=>{:controller=>"/wiki/wikicategorias/busca"} do -%>
        <%= text_field_tag :filtro %>
        <%= submit_tag 'Buscar' %>
      <% end -%>
      <div id="busca_wiki">
      </div>
    </td>
    <td class="itens_index">
      <div id="categorias">
        <%= render(:partial => "categorias") %>
      </div>
    </td>
  </tr>
</table>

