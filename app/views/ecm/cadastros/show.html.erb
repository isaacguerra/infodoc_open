<div id="barra_titulo">
  <h3 class="titulo_show">Exibindo <%= @formulario.titulo %></h3>
  <div id="link_botao"><%= link_to 'Editar', edit_ecm_formulario_cadastro_path(@formulario, @cadastro) %></div>
</div>

<table id="listar">
  <% @formulario.itensformularios.each do |i| %>
  <tr>
    <td class="lista_item"><%= i.rotulo %></td>
    <td class="lista_conteudo"><%= ecm_show_form(i.itenstipo.componente, i, @cadastro.busca_form_item(i)) %></td>
  </tr>
  <% end %>
</table>

<div id="link_voltar">
  <%= link_to 'Voltar', ecm_formulario_cadastros_path(@formulario) %>
</div>

