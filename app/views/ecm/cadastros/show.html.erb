<div id="barra_titulo">
  <h3 class="titulo_show">Exibindo <%= @formulario.titulo %></h3>
  <% if @formulario.permissao(@sessao_usuario.usuario) > 1 %>
  <div id="link_botao"><%= link_to 'Editar', edit_ecm_formulario_cadastro_path(@formulario, @cadastro) %></div>
  <% end %>
</div>

<table id="listar">
  <% @formulario.itensformularios.each do |form_item| %>
  <tr>
    <td class="lista_item"><%= form_item.rotulo %></td>
    <td class="lista_conteudo"><%= ecm_show_cadastro_item(form_item, @cadastro.busca_form_item(form_item)) %></td>
  </tr>
  <% end %>
</table>

<div id="link_voltar">
  <%= link_to 'Voltar', ecm_formulario_cadastros_path(@formulario) %>
</div>

