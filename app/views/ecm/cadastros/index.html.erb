<div id="barra_titulo">
  <h3 class="titulo_index">Lista de <%= @formulario.titulo %></h3>
  <div class="ajuda">
    <%= link_to "Ajuda", ecm_formulario_ajudas_formulario_path(@formulario,1), :popup=>true %>
  </div>
  <% if @formulario.permissao(@sessao_usuario.usuario) > 1 %>
  <div class="novo_cadastro">
    <%= link_to 'Novo Cadastro', new_ecm_formulario_cadastro_path(@formulario) %>
  </div>
  <% end %>
</div>
<div id="busca">
  <%= render :partial=>"busca" %>
</div>
<table id="listar">
    <div id="cadastros">
    </div>
</table>

